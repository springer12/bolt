{
  "rules": {
    "parents": {
      "$key1": {
        ".validate": "newData.hasChildren()",
        "children": {
          "$key2": {
            ".validate": "newData.hasChildren(['age'])",
            "age": {
              ".validate": "newData.isNumber()"
            },
            "$other": {
              ".validate": "false"
            }
          }
        },
        "$other": {
          ".validate": "false"
        }
      }
    }
  }
}
